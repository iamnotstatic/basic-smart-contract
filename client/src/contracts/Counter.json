  "source": "pragma solidity ^0.5.11;\n\ncontract Counter {\n    uint public count;\n\n    constructor (uint _count) public {\n        count = _count;\n    }\n\n\n    function increment () external {\n        count++;\n    }\n\n    function decrement() external {\n        count--;\n    }\n\n\n}",
